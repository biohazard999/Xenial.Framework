{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "docs/**/**.md",
          "docs/**/**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docs/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "docs/apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "artifacts/docs.xenial.io/**"
        ]
      }
    ],
    "dest": "artifacts/docs.xenial.io",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}