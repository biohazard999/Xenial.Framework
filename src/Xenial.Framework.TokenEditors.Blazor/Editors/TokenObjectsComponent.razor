@if (ComponentModel is not null)
{
    <DxTagBox TValue="TokenObjectsDisplayModel"
              TData="TokenObjectsDisplayModel"
              TextFieldName="@nameof(TokenObjectsDisplayModel.DisplayName)"
              Data="@ComponentModel.DataSource"
              Values="@ComponentModel.Values"
              ValuesChanged="ValuesChanged"
              @attributes="@ComponentModel.Attributes"
              ReadOnly="@ComponentModel.ReadOnly">
    </DxTagBox>
}

@code
{
    /// <summary>	Gets or sets the component model. </summary>
    ///
    /// <value>	The component model. </value>

    [Parameter]
    public TokenObjectsInputModel? ComponentModel { get; set; }

    /// <summary>	Creates a new RenderFragment. </summary>
    ///
    /// <param name="componentModel">	The component model. </param>
    ///
    /// <returns>	A RenderFragment. </returns>

    public static RenderFragment Create(TokenObjectsInputModel componentModel) =>@<TokenObjectsComponent ComponentModel="@componentModel" />;

    /// <summary>	Values changed. </summary>
    ///
    /// <param name="values">	The values. </param>

    void ValuesChanged(System.Collections.Generic.IEnumerable<TokenObjectsDisplayModel> values)
    {
        if (ComponentModel is not null)
        {
            ComponentModel.SetValueFromUI(values);
        }
    }
}
