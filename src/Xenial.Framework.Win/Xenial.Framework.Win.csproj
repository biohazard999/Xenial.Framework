  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DevExpress.ExpressApp.Win" Version="$(DxPackageVersion)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Xenial.Framework.Images\Xenial.Framework.Images.csproj" />
    <ProjectReference Include="..\Xenial.Framework\Xenial.Framework.csproj" />
  </ItemGroup>
  
</Project>
