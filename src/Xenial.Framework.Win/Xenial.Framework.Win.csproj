    <UseWindowsForms>true</UseWindowsForms>
    <PackageTags>$(PackageTags) Win</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Layouts\LayoutLabelItem.cs" />
    <Compile Remove="Layouts\LayoutSeparatorItem.cs" />
