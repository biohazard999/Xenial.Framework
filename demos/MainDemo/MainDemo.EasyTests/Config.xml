<?xml version="1.0" encoding="utf-8" ?>
<Options xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" TestRunTimeLimit="3" EnableUpperCase="MainDemoWeb">
  <Applications>
    <!-- Win -->
    <Application
			Name="MainDemoWin"
			FileName="[ConfigPath]\..\MainDemo.Win\Bin\Debug\MainDemo.Win.exe"
			AdapterAssemblyName="DevExpress.ExpressApp.EasyTest.WinAdapter.v21.2, Version=21.2.5.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a"
			CommunicationPort="15923"/>
    <!-- Web -->
    <Application
			Name="MainDemoWeb"
			Url="http://localhost:4030"
			PhysicalPath="[ConfigPath]\..\MainDemo.Web"
			AdapterFileName="[WebAdapterAssemblyPath]"
      IgnoreCase="true"/>
    <!-- For IIS -->
    <!--
    <Application
			Name="MainDemoWeb"
			Url="http://localhost/MainDemo.Web/Default.aspx"
			AdapterFileName="[WebAdapterAssemblyPath]"
			IgnoreCase="true" />
    -->
  </Applications>
  <TestDatabases>
    <Database xsi:type="TestMSSQLDatabase" Server="(localdb)\mssqllocaldb" Name="MainDemo" DBName="MainDemo_v21.2"/>
  </TestDatabases>
  <Aliases>
    <Alias Name="WebAdapterAssemblyPath" Value="[ConfigPath]\..\MainDemo.Web\Bin\EasyTest\DevExpress.ExpressApp.EasyTest.SeleniumWebAdapter.v21.2.dll" />
  </Aliases>
</Options>
