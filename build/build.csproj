<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks></TargetFrameworks>
    <TargetFramework>net5</TargetFramework>
    <GenerateDocumentationFile>false</GenerateDocumentationFile>
    <IsPackable>false</IsPackable>
    <PublishTrimmed>true</PublishTrimmed>
    <Nullable>disable</Nullable>
    <NoWarn>$(NoWarn);CA1310;CA1303;CA1309;CA1307;CA1014</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Bullseye" Version="3.5.0" />
    <PackageReference Include="SimpleExec" Version="6.3.0" />
    <PackageReference Include="Xenial.Beer" Version="0.0.9" />
    <PackageReference Include="Glob" Version="1.1.8" />
  </ItemGroup>

</Project>
